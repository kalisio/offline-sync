{
  "name": "offline-sync",
  "version": "1.0.0",
  "description": "## Goal",
  "private": true,
  "main": "index.js",
  "type": "module",
  "scripts": {
    "compile": "lerna run compile",
    "install": "yarn run compile",
    "test": "yarn compile && lerna run test --scope server && vitest run --coverage",
    "vitest": "vitest run",
    "dev": "./dev.sh",
    "lint": "lerna run lint",
    "lint:fix": "lerna run lint:fix",
    "update-dependencies": "npm-check-updates -u && lerna exec -- npm-check-updates -u",
    "init": "echo AUTOMERGE_ROOT_DOCUMENT=\\\"$(node example/server/initialize.js)\\\" > example/server/.env",
    "s2s": "cd example/server && NODE_ENV=s2s npm run dev"
  },
  "workspaces": [
    "packages/*",
    "example/*"
  ],
  "keywords": [],
  "author": "Kalisio",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "lerna": "^9.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "npm-check-updates": "^19.1.0"
  }
}
